<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardElevation="5dp"
        app:cardPreventCornerOverlap="true"
        android:background="@color/farmerprofilecardpanel1color"
        android:clickable="true"
        android:visibility="gone">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/seperatorcolor"/>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/farmerprofilecardpanel1color"
                    android:padding="10dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/profileImagedatagrid"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/cow" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/seperatorcolor"/>

                <TextView

                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:autoSizeTextType="uniform"
                    android:padding="10dp"
                    android:textAlignment="center"
                    android:text="TextView1"
                    android:background="@color/farmerprofilecardpanel1color" />

                <LinearLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/seperatorcolor"/>

                <TextView

                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:autoSizeTextType="uniform"
                    android:padding="10dp"
                    android:textAlignment="center"
                    android:text="TextView2"
                    android:background="@color/farmerprofilecardpanel1color" />

                <LinearLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/seperatorcolor"/>

                <TextView

                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:autoSizeTextType="uniform"
                    android:padding="10dp"
                    android:text="TextView3"
                    android:background="@color/farmerprofilecardpanel1color" />

                <LinearLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/seperatorcolor"/>

                <TextView
                    android:id="@+id/farmid"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:autoSizeTextType="uniform"
                    android:textAlignment="center"
                    android:padding="10dp"
                    android:text="TextView4"
                    android:background="@color/farmerprofilecardpanel1color" />

                <LinearLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/seperatorcolor"/>

                <TextView

                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:autoSizeTextType="uniform"
                    android:textAlignment="center"
                    android:padding="10dp"
                    android:text="TextView5"
                    android:background="@color/farmerprofilecardpanel1color" />

                <LinearLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/seperatorcolor"/>

                <TextView
                    android:id="@+id/cardStatus"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:autoSizeTextType="uniform"
                    android:textAlignment="center"
                    android:padding="10dp"
                    android:text="TextView6"
                    android:background="@color/farmerprofilecardpanel1color" />


                <LinearLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/seperatorcolor"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:gravity="center_horizontal"
                    android:background="@color/farmerprofilecardpanel1color"
                    android:layout_weight="1"
                    android:orientation="horizontal">
                    <com.google.android.material.button.MaterialButton
                        android:layout_width="500dp"
                        android:layout_height="wrap_content"
                        android:text="Edit"
                        android:layout_weight="1"
                        android:drawableEnd="@drawable/baseline_edit_24_white"
                        android:drawableTint="@color/textcolor"
                        android:textColor="@color/btncolortext"
                        android:backgroundTint="@color/lightblue" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/seperatorcolor"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:gravity="center_horizontal"
                    android:background="@color/farmerprofilecardpanel1color"
                    android:layout_weight="1"
                    android:orientation="horizontal">
                    <com.google.android.material.button.MaterialButton
                        android:layout_width="500dp"
                        android:layout_height="wrap_content"
                        android:text="Milk Cycle"
                        android:layout_weight="1"
                        android:drawableTint="@color/textcolor"
                        android:textColor="@color/btncolortext"
                        android:backgroundTint="@color/lightblue" />


                </LinearLayout>


                <LinearLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/seperatorcolor"/>
            </LinearLayout>




        </LinearLayout>


    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        app:cardElevation="5dp"
        app:cardUseCompatPadding="true"
        app:cardCornerRadius="15dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/backgroundColorpanel"
            android:orientation="horizontal">



            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:orientation="horizontal">




                <LinearLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent" />

                <TextView
                    android:id="@+id/farmerEntityId"
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:layout_marginStart="15dp"
                    android:text="Farmer Entity ID"
                    android:textColor="@color/textcolor"
                    android:textSize="16sp" />




                <LinearLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent" />


                <TextView
                    android:id="@+id/created_at"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:text="Added At"
                    android:textColor="@color/textcolor"
                    android:textSize="16sp" />


                <LinearLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent" />

                <TextView
                    android:id="@+id/created_by"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:text="Added By"
                    android:textColor="@color/textcolor"
                    android:textSize="16sp" />

                <LinearLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent" />

                <ImageView
                    android:id="@+id/viewData"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:src="@drawable/baseline_visibility_24"
                    android:textAlignment="center"
                    android:padding="10dp"/>








            </LinearLayout>



        </LinearLayout>



    </androidx.cardview.widget.CardView>

</LinearLayout>